<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        {# Run `composer require symfony/webpack-encore-bundle`
           and uncomment the following Encore helpers to start using Symfony UX #}
        {% block stylesheets %}
            {#{{ encore_entry_link_tags('app') }}#}
            <!-- Axentix CSS minified version -->
        <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/axentix@2.1.1/dist/axentix.min.css">
        <link rel="stylesheet" href="{{absolute_url(asset('css/bootstrap.css'))}}"/>
        <link rel="stylesheet" href="{{absolute_url(asset('css/bootstrap.min.css'))}}"/>
        {% endblock %}

        {% block javascripts %}
            {#{{ encore_entry_script_tags('app') }}#}
        {% endblock %}
    </head>
    <body>
 
<nav class="navbar navbar-expand-lg navbar-dark bg-dark">
  <a class="navbar-brand" href="#">Train</a>
  <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarColor01" aria-controls="navbarColor01" aria-expanded="false" aria-label="Toggle navigation">
    <span class="navbar-toggler-icon"></span>
  </button>

  <div class="collapse navbar-collapse" id="navbarColor01">
    <ul class="navbar-nav mr-auto">
      <li class="nav-item active">
        <a class="nav-link" href="{{ path('app_train_index') }}">TRAIN <span class="sr-only">(current)</span></a>
      </li>
      <li class="nav-item">
        <a class="nav-link" href="{{ path('app_reservation_index') }}">RESERVATION</a>
      </li>
      <li class="nav-item">
        <a class="nav-link" href="{{ path('app_place_index') }}">PLACE</a>
      </li>
      <li class="nav-item">
        <a class="nav-link" href="{{ path('app_itineraire_index') }}">ITINERAIRE</a>
      </li>
    </ul>
    <form class="form-inline my-2 my-lg-0">
      <input class="form-control mr-sm-2" type="text" placeholder="Search">
      <button class="btn btn-secondary my-2 my-sm-0" type="submit">Recherche</button>
    </form>
  </div>
</nav>

 {% block body %}
        {% endblock %}
 <br> <br> <br> <br> <br> <br> <br> <br> <br>               
